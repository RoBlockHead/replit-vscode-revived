{
  "name": "replit",
  "displayName": "Replit for VSCode",
  "description": "VS Code extension for connecting to repls",
  "icon": "images/replit-vs-code-logo.png",
  "version": "0.8.2",
  "publisher": "miroreo",
  "repository": {
    "type": "git",
    "url": "https://github.com/roblockhead/replit-vscode-revived.git"
  },
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onFileSystem:replit",
    "onCommand:replit.openrepl",
    "onCommand:replit.apikey",
    "onCommand:replit.captchakey"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsWelcome": [
      {
        "view": "replit",
        "contents": "No node dependencies found [learn more](https://www.npmjs.com/).\n[Add Dependency](command:nodeDependencies.addEntry)"
      }
    ],
    "commands": [
      {
        "command": "replit.openrepl",
        "title": "Open a repl",
        "category": "Replit"
      },
      {
        "command": "replit.shell",
        "title": "Open a shell for a repl",
        "category": "Replit"
      },
      {
        "command": "replit.apikey",
        "title": "Add or change API key",
        "category": "Replit"
      },
      {
        "command": "replit.captchakey",
        "title": "Set the CAPTCHA key",
        "category": "Replit"
      },
      {
        "command": "replit.run",
        "title": "Run a repl",
        "category": "Replit",
        "icon": {
          "light": "images/run.svg",
          "dark": "images/run.svg"
        }
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "replit.shell",
          "when": "workbenchState == workspace"
        }
      ],
      "editor/title/run": [
        {
          "command": "replit.run",
          "when": "workbenchState == workspace"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run lint && npm run tscc && npm run build",
    "build": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "lint": "eslint ./src --quiet",
    "tscc": "tsc -p ./ --noEmit",
    "test": "echo \"no teste :(\""
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "printWidth": 100,
    "bracketSpacing": true,
    "arrowParens": "always"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/lodash.debounce": "^4.0.6",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.19.12",
    "@types/node-fetch": "^2.5.7",
    "@types/vscode": "^1.56.0",
    "@types/websocket": "^1.0.1",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "prettier": "^2.2.1",
    "ts-loader": "^9.2.1",
    "typescript": "^4.1.2",
    "vscode-test": "^1.5.2",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.3.1"
  },
  "dependencies": {
    "@replit/crosis": "^6.0.4",
    "@replit/protocol": "^0.2.16",
    "graphql": "^15.4.0",
    "graphql-request": "^3.4.0",
    "lodash.debounce": "^4.0.8",
    "node-fetch": "^2.6.1",
    "ws": "^7.4.2"
  }
}
